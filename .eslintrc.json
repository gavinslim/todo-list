{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "js": "always",
        "no-console": "off",
        "quotes": [
            "error",
            "double"
        ]
    }
}
